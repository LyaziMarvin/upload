{
  "name": "family-circle",
  "version": "1.0.2",
  "description": "Electron desktop app with better-sqlite3",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^25.9.8",
    "electron-builder": "^24.13.1",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.15.1",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^8.7.0",
    "fetch-node": "^0.0.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "pdf-parse": "^1.1.1",
    "uuid": "^11.1.0"
  },
  "build": {
    "appId": "com.family.circle",
    "productName": "Family Circle",
    "files": [
      "public/**/*",
      "app/**/*",
      "src/**/*",
      "assets/icons/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    },
    "asarUnpack": [
      "app/database/**",
      "node_modules/better-sqlite3/**",
      "node_modules/@xenova/**",
      "node_modules/bcrypt/**"
    ],
    "icon": "assets/icons/family-circle",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icons/family-circle.ico"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "assets/icons/family-circle.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "assets/icons/family-circle.png"
    }
  }
}